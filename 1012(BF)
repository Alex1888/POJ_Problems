#include <iostream>
using namespace std;

int main() {

	int res[14] = {0, 2, 7, 5, 30, 169, 441, 1872, 7632, 1740, 93313, 459901, 1358657, 2504881};
	while(1){
		int k;
		cin >> k;
		if(k==0) return k;
		cout << res[k] << endl;
	}
	//cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	return 0;
}
